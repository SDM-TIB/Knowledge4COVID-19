@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix covid-19: <http://covid-19.tib.eu/vocab/> .

<Enzyme>
	rml:logicalSource [ rml:source "/data/drug/enzyme.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/entity/{Enzyme_name}";
		rr:class covid-19:Enzyme
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasEnzymeID;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/entity/{Enzyme_ID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:hasCUIAnnotation;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/entity/{CUI}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:uniprotID;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/entity/{Uni_ID}";
		]
	];
	
	rr:predicateObjectMap [
		rr:predicate covid-19:externalLink;
		rr:objectMap [
			rr:template "https://www.uniprot.org/uniprot/{Uni_ID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:enzyme_isRelatedTo_gene;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/entity/{Gene_name}";
		]
	].

