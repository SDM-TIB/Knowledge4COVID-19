@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix covid-19: <http://covid-19.tib.eu/vocab/> .

<DrugEnzymeInteraction>
	rml:logicalSource [ rml:source "/data/drug/drug_enzyme_interaction.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://covid-19.tib.eu/entity/{Drug_ID}_interactsWith_{Enzyme_name}";
		rr:class covid-19:DrugEnzymeInteraction
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:interactor1;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/entity/{Drug_ID}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate covid-19:interactor2;
		rr:objectMap [ 
			rr:template "http://covid-19.tib.eu/entity/{Enzyme_name}" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate prov:hadPrimarySource;
		rr:objectMap [
			rr:template "http://covid-19.tib.eu/SourceName/KEGG"
		]
	].
